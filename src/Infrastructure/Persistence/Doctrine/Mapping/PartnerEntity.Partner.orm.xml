<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xmlns:gedmo="http://gediminasm.org/schemas/orm/doctrine-extensions-mapping">

    <entity name="App\Domain\Entity\PartnerEntity\Partner" table="s_partner">
        <id name="id" type="integer" column="nu_seq_id">
            <generator strategy="AUTO"/>
        </id>
        <field name="tradingName" column="des_trading_name" />
        <field name="ownerName" column="des_owner_name" />
        <field name="document" column="des_document" />
        <unique-constraints>
            <unique-constraint columns="des_document" name="document_key" />
        </unique-constraints>
        <field name="coverageArea" column="geo_coverage_area" type="multipolygon" />
        <field name="address" column="geo_address" type="point" />
    </entity>

</doctrine-mapping>