<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xmlns:gedmo="http://gediminasm.org/schemas/orm/doctrine-extensions-mapping">

    <entity name="App\Entity\PartnerEntity\Partner" table="s_partner">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="tradingName" column="tradingName" />
        <field name="ownerName" column="ownerName" />
        <field name="document" column="document" />
        <field name="coverageArea" column="coverageArea" type="multipolygon" />
        <field name="address" column="address" type="point" />
    </entity>

</doctrine-mapping>